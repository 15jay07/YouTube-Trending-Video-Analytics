    "# 5
    "from vaderSentiment.vaderSentiment import SentimentIntensityAnalyzer\n",
    "an = SentimentIntensityAnalyzer()\n",
    "df['title_sentiment'] = df['title_clean'].apply(lambda t: an.polarity_scores(t)['compound'])\n",
    "df['tags_joined'] = df['tags_list'].apply(lambda L: ' '.join(L))\n",
    "df['tags_sentiment'] = df['tags_joined'].apply(lambda t: an.polarity_scores(t)['compound'])\n"
   ]
