   "source": [
    "# 2
    "def parse_trending(x):\n",
    "    # Many YouTube trending datasets use 'YY.DD.MM' (e.g. '17.14.11' -> 2017-11-14)\n",
    "    if pd.isna(x): \n",
    "        return pd.NaT\n",
    "    s = str(x).strip()\n",
    "    if '.' in s:\n",
    "        parts = s.split('.')\n",
    "        if len(parts)==3 and len(parts[0])==2:\n",
    "            yy, dd, mm = parts\n",
    "            return pd.to_datetime(f\"20{yy}-{mm}-{dd}\", errors='coerce')\n",
    "    return pd.to_datetime(s, errors='coerce')\n",
    "\n",
    "df['trending_date'] = df['trending_date'].apply(parse_trending)\n",
    "df['publish_date'] = pd.to_datetime(df['publish_date'], errors='coerce')\n",
    "\n",
    "# keep important columns (add or remove as your CSV dictates)\n",
    "keep = ['video_id','trending_date','publish_date','title','channel_title',\n",
    "        'category_id','views','likes','dislikes','comment_count','tags']\n",
    "df = df[[c for c in keep if c in df.columns]]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "9257b2dc-6a81-468f-918b-63e09818bbca",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 3
    "df['title_clean'] = df['title'].fillna('').str.lower().str.strip()\n",
    "df['tags'] = df['tags'].fillna('').astype(str)\n",
    "df['tags_list'] = df['tags'].str.replace('\"','').str.split('|').apply(lambda L: [t.strip().lower() for t in L if t.strip()!=''])\n",
    "\n",
    "# drop rows missing core fields\n",
    "df = df[df['video_id'].notna() & df['trending_date'].notna() & df['views'].notna()]\n",
    "\n",
    "# if multiple rows per (video_id, trending_date) keep row with max views\n",
    "df = df.sort_values(['video_id','trending_date','views']).drop_duplicates(subset=['video_id','trending_date'], keep='last')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "6c146f96-b033-4fbb-8953-8b308b44362a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 4
    "df['days_since_publish'] = (df['trending_date'] - df['publish_date']).dt.days\n",
    "df['trending_day'] = df['trending_date'].dt.date\n",
    "# trending duration per video (later)\n",
    "dur = df.groupby('video_id')['trending_date'].agg(['min','max']).reset_index()\n",
    "dur['trending_duration_days'] = (dur['max'] - dur['min']).dt.days + 1\n",
    "df = df.merge(dur[['video_id','trending_duration_days']], on='video_id', how='left')\n"
   ]
  },
